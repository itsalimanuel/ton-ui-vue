    "@vercel/analytics": "^1.3.1",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "nx": "19.5.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.4",
    "vue-tsc": "^2.0.24"
  },
  "nx": {
    "tags": [
      "scope:ui-vue"
